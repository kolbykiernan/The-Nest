import { Router } from 'express';

const router = Router();

//gets you tp questionnaire//
router.get('/', (req, res) => {
    res.send('GET /questionnaire')
})

//submits answers to db
router.post('/', (req, res) => {
    res.send('Post /questionnaire')
})

router.get('/categories', (req, res) => {
    res.send('GET /categories')
})


export { router };